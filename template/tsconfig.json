{
  "extends": "@react-native/typescript-config",
  "compilerOptions": {
    "jsx": "react",
    "target": "es2017",
    "noImplicitAny": true,
    "strict": false,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "strictNullChecks": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}